<% include partials/header %>



<div class="container mt-5">
<div class="row">
    <div class="col-md-12">
        
        <h1>Please Login</h1>
        
        <form action="/login" method="POST">

            <div class="form-group">
                <label for="username">User Name</label>
                <input type="text" class="form-control shadow-lg p-3 mb-5 bg-white rounded" name="username" placeholder="username">
            </div>

            <div class="form-group">
                <label for="password">Enter Password</label>
                <input type="password" class="form-control shadow-lg p-3 mb-5 bg-white rounded" name="password" placeholder="password">
            </div>
            <button class="btn btn-primary">Login</button>
        </form>

    </div>
</div>   
</div>













